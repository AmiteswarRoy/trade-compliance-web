{
  "name": "trade-compliance-web",
  "version": "0.0.0",
  "description": "An ES7 isomorphic Flux/ReactJS project",
  "main": "server/index.js",
  "scripts": {
    "build": "npm run clean && npm run b-staging && npm run b-integration && npm run b-production",
    "b-integration": "cross-env BABEL_ENV=integration babel-node ./node_modules/.bin/webpack --stats --config ./webpack/env.config.js",
    "b-production": "cross-env BABEL_ENV=production babel-node ./node_modules/.bin/webpack --stats --config ./webpack/env.config.js",
    "b-staging": "cross-env BABEL_ENV=staging babel-node ./node_modules/.bin/webpack --stats --config ./webpack/env.config.js",
    "clean": "rimraf dist*",
    "dev": "cross-env VIRTUAL_HOST=127.0.0.1 BABEL_ENV=development babel-node ./webpack/dev-server.js",
    "mvp": "cross-env VIRTUAL_HOST=10.147.135.112 BABEL_ENV=development babel-node ./webpack/mvp-server.js",
    "dev-debug": "cross-env babel-node-debug ./webpack/dev-server.js",
    "test": "npm run lint && cross-env NODE_ENV=test nyc mocha \"test/spec/**/*.test.js\"",
    "test:watch": "cross-env NODE_ENV=test nyc mocha -w \"test/spec/**/*.test.js\"",
    "test:report": "cross-env NODE_ENV=test nyc mocha --reporter mochawesome \"test/spec/**/*.test.js\"",
    "test:cover": "rimraf coverage && cross-env NODE_ENV=test nyc mocha \"test/spec/**/*.test.js\"",
    "lint": "eslint app server shared test --ext .jsx,.js",
    "djvs-i18n-generator": "djini18ngenerator --productId symphony --outputPath ./app/i18n && renamer --find 'zhtw' --replace 'zh-Hant-TW' ./app/i18n/*.js && renamer --find 'zhcn' --replace 'zh-Hans-CN' ./app/i18n/*.js",
    "update-tokens": "npm run djvs-i18n-generator",
    "rename": "renamer --find './app/i18n/zhtw.js' --replace'./app/i18n/zh-Hant-TW.js'",
    "install-deps": "rimraf node_modules && npm i --registry=https://artifactory.dowjones.io/artifactory/api/npm/npm-virtual/",
    "install-deps-win": "npm i djvs-util-i18n-dictionary-generator --registry=https://artifactory.dowjones.io/artifactory/api/npm/npm-virtual/ && npm i --msvs-version=2013",
    "outdated-deps": "npm outdated --registry=https://artifactory.dowjones.io/artifactory/api/npm/npm-virtual/",
    "production": "npm run b-production && cross-env UV_THREADPOOL_SIZE=100 NODE_ENV=production SSL=off DEBUG=dev,koa node ./server/index.js",
    "staging": "npm run b-staging && cross-env UV_THREADPOOL_SIZE=100 NODE_ENV=staging SSL=off DEBUG=dev,koa node ./server/index.js",
    "integration": "npm run b-integration && cross-env UV_THREADPOOL_SIZE=100 NODE_ENV=integration SSL=off DEBUG=dev,koa node ./server/index.js",
    "build-win": "npm run clean && npm run b-production-win",
    "b-production-win": "cross-env VIRTUAL_HOST=10.1.106.70 PORT=2999 BABEL_ENV=production babel-node -- ./node_modules/webpack/bin/webpack.js --stats --config ./webpack/prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AmiteswarRoy/trade-compliance-web.git"
  },
  "homepage": "https://github.com/AmiteswarRoy/trade-compliance-web.git",
  "bugs": "https://github.com/AmiteswarRoy/trade-compliance-web.git",
  "author": "iam4x",
  "license": "MIT",
  "nyc": {
    "sourceMap": false,
    "instrument": false,
    "extension": [
      ".jsx",
      ".es6"
    ],
    "reporter": [
      "html",
      "text-summary"
    ],
    "exclude": [
      "test/**/*",
      "shared/**/*",
      "server/**/*",
      "app/i18n/*",
      "app/utils/intl-loader.js",
      "app/utils/image-resolver.js",
      "app/utils/localized-routes.jsx"
    ],
    "check-coverage": true,
    "statements": 80,
    "branches": 65,
    "functions": 85,
    "lines": 83,
    "cache": true
  },
